    "@io_bazel_rules_go//go:def.bzl",
    "go_library",
)

go_library(
    name = "go_default_library",
    srcs = [
        "copy.go",
        "equal.go",
        "path_value.go",
        "prettify.go",
        "string_value.go",
    ],
    tags = ["automanaged"],
    deps = ["//vendor/github.com/jmespath/go-jmespath:go_default_library"],
)

